{
    "name": "sedp-mis/base-repository",
    "description": "Abstraction for model repository",
    "keywords": ["repository", "base repository", "model repository"],
    "license": "MIT",
    "authors": [
        {
            "name": "Arjon Jason Castro",
            "email": "ajcastro29@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "laravel/framework": "4.2.11",
        "sedp-mis/lib": "dev-master"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "classmap": [
            "app/database/migrations",
            "app/database/seeds",
            "app/tests/TestCase.php"
        ],
        "psr-4": {
            "SedpMis\\BaseRepository\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "test_classes/"
        ]
    },
    "minimum-stability": "stable"
}
